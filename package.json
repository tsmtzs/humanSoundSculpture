{
  "name": "humansoundsculpture",
  "version": "1.0.0",
  "description": "A web browser version of the piece Human Sound Sculpture",
  "main": "server.js",
  "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "sudo systemctl start hss-webServer",
      "stop": "sudo systemctl stop hss-supercollider",
      "restart": "sudo systemctl restart hss-webServer",
      "status": "systemctl status hss-webServer",
      "status-sc": "systemctl status hss-supercollider",
      "log": "journalctl -u hss-webServer -r",
      "log-sc": "journalctl -u hss-supercollider -r",
      "log-follow": "journalctl -u hss-webServer -f",
      "log-sc-follow": "journalctl -u hss-supercollider -f"
  },
  "author": "Tassos Tsesmetzis",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "node-osc": "^4.1.1",
    "osc": "^2.3.1",
    "ws": "^3.3.3"
  }
}
